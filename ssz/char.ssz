      }
      if(dalpha != .com.IERR){
        dalpha = .m.max!int?(0)<, .m.min!int?(255)<, dalpha;
      }
      if(salpha == 1 && dalpha == 255) salpha = 0;
    case 4://add1
      branch{
      cond salpha != .com.IERR:
        salpha = .m.max!int?(0)<, .m.min!int?(255)<, salpha;
      else:
        salpha = 255;
