    if(`ls(.lsSCREENBOUND)){
      float min, max;
      branch{
      cond `facing > 0:
        min = .com.xmin + `getEdge(`bedge, true);
        max = .com.xmax - `getEdge(`fedge, true);
      else:
        min = .com.xmin + `getEdge(`fedge, true);
        max = .com.xmax - `getEdge(`bedge, true);
