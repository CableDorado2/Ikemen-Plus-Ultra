public ^/char trim(^/char str)
{
  index s, e;
  loop{s = 0; while; do:
    if(!.cMatch!char?(.blank, str[s])) break;
    s++;
  while s < #str:
  }
  loop{e = #str; while; do:
    if(!.cMatch!char?(.blank, str[e-1])) break;
    e--;
  while e > 0:
  }
  ret str[s..e];
}

public %^/char split(^/char dlim, ^/char src)
{
  %^/char spl;
  loop{
    index i = 0, j;
    index l = #dlim > 0 ? 0 : 1;
    continue;
  do:
    spl.new[-1] = src[i..i+j+l];
    i += j + #dlim + l;
  continue:
    j = .find(dlim, src[i..-1]);
  while j >= 0:
    spl.new[-1] = src[i..-1];
  }
  ret spl;
}

public %char join(^/char dlim, ^/^/char src)
{
  %char jol;
  loop{
    index i = 0;
    if(i >= #src) break;
    jol .= src[i];
    continue;
  do:
    jol .= dlim;
    jol .= src[i];
  continue:
    i++;
  while i < #src:
  }
  ret jol;
}

